<configuration>
  <!-- ID of the service. It should be unique accross the Windows system-->
  <id>piston-print</id>
  <!-- Display name of the service -->
  <name>Cloud printer Piston</name>
  <!-- Service description -->
  <description>This server responsible to print cloud</description>
  <!-- Path to the executable, which should be started -->
  <executable>%BASE%\..\piston-print.exe</executable>
  <arguments>server</arguments>
</configuration>
